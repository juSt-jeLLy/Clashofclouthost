{
"version": 2,
  "framework": "nextjs",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ],
  "builds": [
    {
      "src": "client/next.config.ts",
      "use": "@vercel/next",
      "config": {
        "typescript": {
          "ignoreBuildErrors": false,
          "tsconfigPath": "./client/tsconfig.json"
        }
      }
    }
  ]
}
