{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/(.*)",
      "dest": "client/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "client/$1"
    }
  ]
}
